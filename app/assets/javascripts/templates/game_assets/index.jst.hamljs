%h1 Listing Game Assets
%table{width: "100%"}
  %tbody
    %tr
     %th{width: "20%"} Name
     %th Price
     %th User
     %th Buttons

  %tbody
    :each asset in game_assets
      %tr
        %td= asset.get("name")
        %td= asset.get("price")
        %td= asset.get("character_id")
        %td
          != link_to("Show", character_game_asset_path(asset.get("character_id"), asset))
          &nbsp;
          != link_to("Edit", edit_character_game_asset_path(asset.get("character_id"), asset))
          &nbsp;
          != link_to("Destroy", character_game_asset_path(asset.get("character_id"), asset),{confirm: "Are you sure?", method: "delete" })